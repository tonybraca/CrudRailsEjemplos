<!DOCTYPE html>
<html>
<head>
  <title>RailsCrudExample</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
  <div class="container">
    <div style="background-color:#255B99" class="clearfix">
      <img src="<%= asset_path 'cintillo_intranet.jpg' %>" class="d-inline-block align-top " alt="">
    </div>
    <nav class="navbar">
      <a class="navbar-brand" href="#">
        <img src="<%= asset_path 'logo_1.png' %>"  class="d-inline-block align-top" alt="">
      </a>
    </nav>
    <%= render '/home/menu' %>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <div class="row">
      <%= yield %>  
    </div>
    <div class="container footer">
      <div style="text-align: right;">
        <%= t('Llamar_Scada') %>
      </div>
      <div style="text-align: left;">
        <%= t('Llamar_Scada') %>
      </div>

   </div>
 </div>
</body>
</html>
